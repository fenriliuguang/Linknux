/* pages/user/user.wxss */
.type{
    color: #000000;
    position: fixed;
    bottom: 55px;
    background-color: #FFFFFF;
    display: flex-end;
    border-bottom:2px solid #FFFFFF;
    margin-left: 0px;
    padding-right: 130px;
}
.text{
    width: max-content;
    font-size:30px;
    box-sizing:border-box;
    text-align: center;
    border-bottom:2px solid #F8F8F8;
}
.button{
    color: #000000;
    background-color: #FFFFFF;
    border-bottom:2px solid #F8F8F8;
    box-sizing:border-box;
    font-size: large;
}
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    height: 1400rpx;
}