/* pages/document/document.wxss */
page{
  width:100%;
  height: 100%;
}
.date{
  font-size: 10px;
}
.check{
  height: 150px;
  border-bottom:1px solid whitesmoke;
}
.title{
  text-align: center;
  font-size: 20px;
  font-weight: 600;
}
.detail{
  align-items: flex-end;
  display: flex;
  margin-top: 30px;
  justify-content: space-around;
}
.user{
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.wechat{
  border-radius: 50%;
  margin-right: 5px;
  width: 50px;
  height: 50px;
}
.gz{
  text-align: center;
  font-size: 15px;
  box-shadow: 1px 1px 1px rgb(9, 196, 196);
  line-height: 35px;
  width: 60px;
  background-color: yellow;
  height: calc(60 * 0.618px);
  border-radius: 7px;
}